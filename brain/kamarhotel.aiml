<?xml version="1.0" encoding="UTF-8"?>
<aiml>

    <category>
        <pattern>KAMAR HOTEL</pattern>
        <template>
            <think><set name="sedia">sedia</set></think>
            Hotel Santika Premiere menyediakan berbagai jenis kamar
            [ul]
                [li]Deluxe[/li]
                [li]Club Premiere[/li]
                [li]Deluxe Suite[/li]
                [li]Executive Suite[/li]
                [li]Family Suite[/li]
                [li]Santika Suite[/li]
                [footer]Info lain[/footer]    
            [/ul]
        </template>
    </category>

    <category>
        <pattern>DELUXE</pattern>
        <template>
            <get name="sedia/>
            <condition name = "sedia" value = "sedia">
                Spesifikasi Kamar Hotel Deluxe adalah:
                [ul]
                    [li]1 Kamar untuk 2 Orang.[/li]
                    [li]Terdapat dua jenis kamar, yaitu King Size (1 tempat tidur) dan Twin Size (2 tempat tidur).[/li]
                    [li]Disediakan Standing Shower.[/li]
                    [li]Disediakan sarapan jam 6-10 pagi untuk dua orang per kamar.[/li]
                    [li]Untuk informasi Smoking Room dapat langsung bertanya ke resepsionis saat menentukan kamar.[/li]
                [/ul]
            </condition>
            <condition name = "sedia" value = "kamar">
                <think><set name="kamar">deluxe</set></think>
                <think><set name="sedia">tanggal</set></think>
                <!-- <get name="kamar"/> -->
                <srai>TANGGAL</srai>
            </condition>
        </template>
    </category>

    <category>
        <pattern>DELUXE SUITE</pattern>
        <template>
            <condition name = "sedia" value = "sedia">
                Spesifikasi Kamar Hotel Deluxe Suite adalah:
                [ul]
                    [li]1 Kamar untuk 2 Orang.[/li]
                    [li]Terdapat dua jenis kamar, yaitu King Size (1 tempat tidur) dan Twin Size (2 tempat tidur).[/li]
                    [li]Disediakan Standing Shower dan bathrobe.[/li]
                    [li]Memiliki Living Room[/li]
                    [li]Disediakan sarapan jam 6-10 pagi untuk dua orang per kamar.[/li]
                    [li]Disediakan afternoon tea / coffee break jam 5 sore - 9 malam untuk dua orang per kamar.[/li]
                    [li]Minibar di kamar dapat digunakan secara gratis.[/li]
                    [li]Disediakan jasa laundry untuk 4 potong pakaian per hari per kamar.[/li]
                    [li]Disediakan setrika dan papan setrika.[/li]
                    [li]Dapat menggunakan bussiness center untuk 2 jam perhari per kamar.[/li]
                    [li]Untuk informasi Smoking Room dapat langsung bertanya ke resepsionis saat menentukan kamar.[/li]
                [/ul]
            </condition>
            <condition name = "sedia" value = "kamar">
                <think><set name="kamar">deluxe suite</set></think>
                <think><set name="sedia">tanggal</set></think>
                <!-- <get name="kamar"/> -->
                <srai>TANGGAL</srai>
            </condition>
        </template>
    </category>

